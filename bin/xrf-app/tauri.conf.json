{
  "productName": "xrf-app",
  "version": "0.1.0",
  "identifier": "com.xrf.tools",
  "build": {
    "beforeDevCommand": "npm run --prefix xrf-ui dev",
    "beforeBuildCommand": "npm run --prefix xrf-ui build",
    "frontendDist": "../xrf-ui/target",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {},
  "app": {
    "windows": [
      {
        "title": "xrf tools",
        "width": 800,
        "height": 600,
        "minWidth": 400,
        "minHeight": 400,
        "resizable": true
      }
    ]
  }
}